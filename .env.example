NEXT_PUBLIC_FEATURE_REALLOCATION_ENABLED=true
NEXT_PUBLIC_FEATURE_CROSS_SUB_MATCH_ENABLED=true
NEXT_PUBLIC_FEATURE_CONSOLIDATION_ENABLED=true

# Mock Data Settings
NEXT_PUBLIC_USE_MOCK_DATA=true
